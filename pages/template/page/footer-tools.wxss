.footer-tools{
    position: fixed;
    bottom: 0;
    left: 0;
    
    height: 145rpx;
    width: 750rpx;
    background-color: none;
    box-sizing: border-box;
    text-align: center;
    z-index: 1;
}
.footer-tools .footer-bottom-gb{
  height: 100rpx;
  background:#F0F0F0;/*#f9f9fa;*/
  width: 750rpx;
  position: absolute;
  bottom: 0;
  left: 0;
  padding: 20rpx 100rpx 25rpx 100rpx;
  box-sizing: border-box;
  z-index: -1;
  border-top: 1rpx solid #e3e3e3;
  box-shadow:0 -2rpx 4rpx 0 rgba(227,227,227,0.50);
}
.footer-tools .discovery{
  display: block;
  text-align: center;
  position: absolute;
  left: 15rpx;
  top:45rpx;
  padding: 20rpx 85rpx;
}

.footer-tools .account{
  display: block;
  text-align: center;
  position: absolute;
  right: 15rpx;
  top:45rpx;
  padding: 20rpx 85rpx;
}
.footer-tools .discovery .title,.footer-tools .account .title{
  font-size: 20rpx;
  text-align: center;
  color: #6f6f70;
  margin-left: -4rpx;
}
.footer-tools .discovery .title.active,.footer-tools .account .title.active{
  font-size: 20rpx;
  text-align: center;
  color: #bb9569;
  margin-left: -4rpx;
}
.footer-tools .add{
  height: 120rpx;
  width: 120rpx;
  display: inline-block;
  background-color: #fff;
  border-radius: 120rpx;
  padding-top: 40rpx;
  box-sizing: border-box;
}
.my-warn-new{
  top:20rpx;
  left: 115rpx;
}